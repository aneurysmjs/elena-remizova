{
  "extends": "stylelint-prettier/recommended",
  "plugins": ["stylelint-prettier", "stylelint-scss"],
  "syntax": "scss",
  "rules": {
    "prettier/prettier": true,
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "tailwind"
      ]
    }],
    "scss/at-rule-no-unknown": true,
    "block-no-empty": null,
    "unit-whitelist": ["em", "rem", "s", "px", "fr"]
  }
}